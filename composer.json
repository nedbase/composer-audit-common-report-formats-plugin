{
    "name": "nedbase/composer-audit-junit-plugin",
    "type": "composer-plugin",
    "version": "1.0.0",
    "description": "JUnit formatted output for the composer audit command",
    "license": "MIT",
    "authors": [
        {
            "name": "Leon Boot",
            "email": "leon@nedbase.nl"
        }
    ],
    "require-dev": {
        "composer/composer": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Nedbase\\Composer\\": "src"
        }
    },
    "extra": {
        "class": "Nedbase\\Composer\\JUnitAuditPlugin"
    },
    "require": {
        "php": "^7.2.5 || ^8.0",
        "ext-json": "*",
        "ext-dom": "*",
        "composer-plugin-api": "^2.4"
    }
}
