{
    "name": "nedbase/composer-audit-common-report-formats-plugin",
    "type": "composer-plugin",
    "keywords": ["composer", "audit", "junit", "trivy", "reports", "ci"],
    "description": "Common report formats for the composer audit command, usable for CI processes",
    "license": "MIT",
    "authors": [
        {
            "name": "Leon Boot",
            "email": "leon@nedbase.nl"
        }
    ],
    "require-dev": {
        "composer/composer": "^2.4",
        "phpunit/phpunit-dom-assertions": "^2.6",
        "phpunit/phpunit": "^8.5",
        "vimeo/psalm": "^4.30"
    },
    "autoload": {
        "psr-4": {
            "Nedbase\\Composer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nedbase\\Test\\Composer\\": "tests"
        }
    },
    "extra": {
        "class": "Nedbase\\Composer\\CommonReportFormatsAuditPlugin"
    },
    "require": {
        "php": "^7.2.5 || ^8.0",
        "ext-json": "*",
        "ext-dom": "*",
        "composer-plugin-api": "^2.3"
    }
}
